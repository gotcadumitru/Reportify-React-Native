{
  "name": "reportify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "splash": "react-native generate-bootsplash src/assets/logo.png  --background-color=FFFCFF  --logo-width=200  --assets-path=assets  --flavor=main",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "clean:cache": "react-native start --reset-cache",
    "build:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug && cd ../"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-google-signin/google-signin": "^7.2.2",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-native-picker/picker": "^2.4.0",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@redux-devtools/extension": "^3.2.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "fbjs": "^3.0.4",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^5.0.1",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-blob-util": "^0.15.0",
    "react-native-bootsplash": "^4.1.4",
    "react-native-document-picker": "^8.1.0",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-image-crop-picker": "^0.37.3",
    "react-native-modal": "^13.0.1",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-paper": "^4.12.0",
    "react-native-pdf": "^6.4.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-vector-icons": "^9.1.0",
    "react-native-video": "^5.2.0",
    "react-redux": "^7.2.8",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/runtime": "^7.17.8",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.12.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.70.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
